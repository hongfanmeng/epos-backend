{
  "kind": "collectionType",
  "collectionName": "option_groups",
  "info": {
    "singularName": "option-group",
    "pluralName": "option-groups",
    "displayName": "Product Option Group",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "optionGroups"
    },
    "minCount": {
      "type": "integer",
      "min": 0,
      "required": true,
      "default": 0
    },
    "maxCount": {
      "type": "integer"
    },
    "options": {
      "type": "component",
      "repeatable": true,
      "component": "product.product-option"
    }
  }
}
